{
    "name": "bugbirdco/swift-module",
    "type": "laravel-library",
    "description": "Contained service module for a Swift host and includes Orchid Platform. Based on bugbirdco/quick",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.133",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "orchid/platform": "^7.2",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "src/Database/seeds",
            "src/Database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
